++Definedb[1, 'name']
++key1:value1
++key2:value2
++password:wdndaslkx
++Definedb[2, 'section']
++bobrealname:freddy
++Definedb[2, 'section']
++mydogname:kitkat
++Definedb[3, 'reason for ending']